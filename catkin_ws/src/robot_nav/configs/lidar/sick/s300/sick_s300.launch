<?xml version="1.0"?>
<launch>
<!-- front LiDAR -->

    <!-- parameters -->
    <arg name="name_1" default="lidar_1" />
    <rosparam command="load" ns="$(arg name_1)" file="$(find robot_nav)/configs/lidar/sick/s300/s300_1.yaml"/>
    <!-- start node -->
    <node pkg="sick_s300" ns="$(arg name_1)" type="sick_s300_node" name="scan_node" respawn="false" output="screen" >
    </node>
    <!-- start filter node -->
    <node pkg="sick_s300" ns="$(arg name_1)" type="scan_filter_node" name="scan_filter" respawn="false" output="screen" >
    </node>

<!-- back LiDAR -->

    <arg name="name_2" default="lidar_2" />
    <rosparam command="load" ns="$(arg name_2)" file="$(find robot_nav)/configs/lidar/sick/s300/s300_2.yaml"/>
    <node pkg="sick_s300" ns="$(arg name_2)" type="sick_s300_node" name="scan_node" respawn="false" output="screen" >
    </node>
    <node pkg="sick_s300" ns="$(arg name_2)" type="scan_filter_node" name="scan_filter" respawn="false" output="screen" >
    </node>

</launch>
